{
  "name": "@codelex/engine-oniguruma",
  "type": "module",
  "version": "3.4.0",
  "description": "Engine for Codelex using Oniguruma RegExp engine in WebAssembly",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/deepcode-ai/codelex#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deepcode-ai/codelex.git",
    "directory": "packages/engine-oniguruma"
  },
  "bugs": "https://github.com/deepcode-ai/codelex/issues",
  "keywords": [
    "codelex",
